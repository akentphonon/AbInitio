# Database schema for VNF


CREATE TABLE job (
    id          char(10) PRIMARY KEY,   # Job id
    kind        varchar(128), NOT NULL  # Kind of simulation: "Quantum Espresso", "VASP", "GULP" etc.
    type        varchar(128), NULL      # Type of the simulation
    status      varchar(128), NOT NULL  # Status of the job: "Not started", "Running", "Finished"
    created     varchar(128), NOT NULL  #
    config      varchar(4096), NOT NULL # Configuration text
);

CREATE TABLE matter (

)

CREATE TABLE configuration (
    id


)

CREATE TABLE simulation (
    id


)


