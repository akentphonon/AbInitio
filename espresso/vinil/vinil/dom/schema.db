# Already implemented table in VNF

CREATE TABLE abinitio
(
  engine                character varying(64) DEFAULT 'vasp'::character varying,
  creator               character varying(64) DEFAULT ''::character varying,
  results_state         character varying(16) DEFAULT ''::character varying,
  job                   character varying(64) DEFAULT ''::character varying,
  generateinputsonly    boolean DEFAULT false,
  date                  date,
  id                    character varying(64) NOT NULL DEFAULT ''::character varying,
  kineticenergycutoff   real DEFAULT 140.0,
  matter                character varying(1024),
  short_description     character varying(128) DEFAULT ''::character varying,
  monkhorstpackmesh     integer[],
  xcfunctional          character varying(64) DEFAULT 'PAW-PBE'::character varying,

  CONSTRAINT abinitio_pkey PRIMARY KEY (id)
)


CREATE TABLE acl1
(
  userid                character varying(64) DEFAULT ''::character varying,
  id                    character varying(64) NOT NULL DEFAULT ''::character varying,
  groupid               character varying(64) DEFAULT ''::character varying,

  CONSTRAINT acl1_pkey PRIMARY KEY (id),
  CONSTRAINT acl1_groupid_fkey FOREIGN KEY (groupid)
      REFERENCES roles (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT acl1_userid_fkey FOREIGN KEY (userid)
      REFERENCES users (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)


CREATE TABLE acl2
(
  userid                character varying(64) DEFAULT ''::character varying,
  id                    character varying(64) NOT NULL DEFAULT ''::character varying,
  groupid               character varying(64) DEFAULT ''::character varying,

  CONSTRAINT acl2_pkey PRIMARY KEY (id),
  CONSTRAINT acl2_groupid_fkey FOREIGN KEY (groupid)
      REFERENCES roles (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION,
  CONSTRAINT acl2_userid_fkey FOREIGN KEY (userid)
      REFERENCES roles (id) MATCH SIMPLE
      ON UPDATE NO ACTION ON DELETE NO ACTION
)


CREATE TABLE acl_instrumentsimulationprivileges
(
  instrument            character varying(64) DEFAULT ''::character varying,
  "role"                character varying(64) DEFAULT ''::character varying,
  id                    character varying(64) NOT NULL DEFAULT ''::character varying,

  CONSTRAINT acl_instrumentsimulationprivileges_pkey PRIMARY KEY (id),
  CONSTRAINT acl_instrumentsimulationprivileges_instrument_fkey FOREIGN KEY (instrument)
